<div id="setting" class="btn-group">

<a class="btn dropdown-toggle" data-toggle="dropdown" href="#">
    Settings <span class="caret"></span>
</a>

<ul class="dropdown-menu pull-right">
    <li>
        <a id="toggle-editor" tabindex="-1" href="#" data-toggle="tooltip" data-placement="left" data-title="<%= editorTitle %>"><i<%= editor ? ' class="icon-ok"' : ''%>></i>Editor</a>
    </li>
    <li>
        <a id="toggle-console" tabindex="-1" href="#" data-toggle="tooltip" data-placement="left" data-title="<%= consoleTitle %>"><i<%= console ? ' class="icon-ok"' : ''%>></i>Console</a>
    </li>
</ul>

</div>
